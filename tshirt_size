def size(cms):
  char return_size ='\0'
  if cms < 38:
    return_size = 'S'
  elif (cms>= 38 and cms <= 42): 
    return_size = 'M'
  else:
      return_size = 'L'
return (return_size)

assert(size(36) == 's')
assert(size(38) == 'm')
assert(size(40) == 'm')
assert(size(45) == 'l')
print("all is welled")
